AK,CAACQ,GAAN,CAAU,yCAAV,CAAN,EAAd,CACAN,OAAO,CAACO,KAAR,CAAcF,OAAd,CAAuB,SAAAG,IAAI,CAAI,CAC7BL,OAAO,CAACM,IAAI,CAACC,SAAL,CAAeF,IAAf,CAAqB,IAArB,CAA2B,CAA3B,CAAD,CAAP,CACD,CAFD,EAGD,CALD,CAMAb,SAAS,CAAC,UAAM,CACdS,QAAQ,GACT,CAFQ,CAEN,EAFM,CAAT,CAIA,MACE,4BAAK,SAAS,CAAC,gBAAf,EACI,4BAAM,SAAS,CAAC,UAAhB,uBADJ,CAEI,2BAAK,SAAS,CAAC,YAAf,EACI,+BACE,oBAAC,OAAD,EAAS,OAAO,CAAEJ,OAAlB,EACI,gCAAOE,IAAP,CADJ,CADF,CADJ,CAFJ,CASI,2BAAK,SAAS,CAAC,WAAf,EACE,yBAAG,SAAS,CAAC,mBAAb,CAAiC,OAAO,CAAEE,QAA1C,YADF,CATJ,CADF,CAiBD,CAjCD,CAmCA,cAAeL,CAAAA,eAAf,C","sourcesContent":["import React, { useState, useEffect } from 'react';\nimport { useFetcher, Fetcher } from 'use-fetcher-react';\nimport axios from 'axios';\n\nconst LocationExample = () => {\n  const fetcher = useFetcher({\n    adjustBorderRadius: true\n  });\n  const [json, setJson] = useState('No result yet');\n\n  const loadData = () => {\n    let request = () => axios.get('https://api.chucknorris.io/jokes/random');\n    fetcher.fetch(request, data => {\n      setJson(JSON.stringify(data, null, 2));\n    });\n  };\n  useEffect(() => {\n    loadData();\n  }, []);\n\n  return (\n    <div className=\"test-container\">\n        <span className=\"my-title\">Random JSON loader</span>\n        <div className=\"my-content\">\n            <pre>\n              <Fetcher fetcher={fetcher}>\n                  <code>{json}</code>\n              </Fetcher>\n            </pre>\n        </div>\n        <div className=\"my-footer\">\n          <a className=\"button is-primary\" onClick={loadData}>\n            Refresh\n          </a>\n        </div>\n    </div>\n  );\n};\n\nexport default LocationExample;\n"]}]}