C,CAAAA,OAAO,CAAG,QAAVA,CAAAA,OAAU,SAAMT,CAAAA,KAAK,CAACU,GAAN,CAAU,yCAAV,CAAN,EAAd,CACAR,OAAO,CAACS,KAAR,CAAcF,OAAd,CAAuB,SAAAG,IAAI,CAAI,CAC7BL,OAAO,CAACM,IAAI,CAACC,SAAL,CAAeF,IAAf,CAAqB,IAArB,CAA2B,CAA3B,CAAD,CAAP,CACD,CAFD,EAGD,CALD,CAMAf,SAAS,CAAC,UAAM,CACdW,QAAQ,GACT,CAFQ,CAEN,EAFM,CAAT,CAIA,MACE,4BAAK,SAAS,CAAC,gBAAf,EACE,oBAAC,OAAD,EAAS,OAAO,CAAEN,OAAlB,EACE,4BAAM,SAAS,CAAC,UAAhB,uBADF,CAEE,2BAAK,SAAS,CAAC,YAAf,EACE,+BACE,gCAAOI,IAAP,CADF,CADF,CAFF,CAOE,2BAAK,SAAS,CAAC,WAAf,EACE,yBAAG,SAAS,CAAC,mBAAb,CAAiC,OAAO,CAAEE,QAA1C,YADF,CAPF,CADF,CADF,CAiBD,CApCD,CAsCA,cAAeP,CAAAA,eAAf,C","sourcesContent":["import React, { useState, useEffect } from 'react';\nimport { useFetcher, Fetcher } from 'use-fetcher-react';\nimport axios from 'axios';\n\nconst ProgressExample = () => {\n  const fetcher = useFetcher({\n    progress: {\n      show: true\n    },\n    minDelay: 2000\n  });\n  const [json, setJson] = useState('No result yet');\n\n  const loadData = () => {\n    let request = () => axios.get('https://api.chucknorris.io/jokes/random');\n    fetcher.fetch(request, data => {\n      setJson(JSON.stringify(data, null, 2));\n    });\n  };\n  useEffect(() => {\n    loadData();\n  }, []);\n\n  return (\n    <div className=\"test-container\">\n      <Fetcher fetcher={fetcher}>\n        <span className=\"my-title\">Random JSON loader</span>\n        <div className=\"my-content\">\n          <pre>\n            <code>{json}</code>\n          </pre>\n        </div>\n        <div className=\"my-footer\">\n          <a className=\"button is-primary\" onClick={loadData}>\n            Refresh\n          </a>\n        </div>\n      </Fetcher>\n    </div>\n  );\n};\n\nexport default ProgressExample;\n"]}]}