{
  "name": "demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.17",
    "@types/node": "12.7.1",
    "@types/react": "16.9.1",
    "@types/react-dom": "16.8.5",
    "bulma": "^0.7.5",
    "react": "../node_modules/react/",
    "react-dom": "../node_modules/react-dom/",
    "react-scripts": "3.1.1",
    "typescript": "3.5.3",
    "raw-loader": "^3.0.0",
    "react-fetcher-hooks": "link:.."
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docz:dev": "docz dev --typescript --port=4838",
    "docz:build": "docz build",
    "switch:local": "rm -rf ./node_modules && rm package.json && cp package-local.json package.json && yarn install && yarn start",
    "switch:prod": "rm -rf ./node_modules && rm package.json && cp package-prod.json package.json && yarn install && yarn docz:dev"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
