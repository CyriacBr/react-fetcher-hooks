C,WALW,eAO5B,GAAMC,CAAAA,QAAQ,CAAG,QAAXA,CAAAA,QAAW,EAAM,CACrB,GAAIC,CAAAA,OAAO,CAAG,QAAVA,CAAAA,OAAU,SAAMV,CAAAA,KAAK,CAACW,GAAN,CAAU,yCAAV,CAAN,EAAd,CACAT,OAAO,CAACU,KAAR,CAAcF,OAAd,CAAuB,SAAAG,IAAI,CAAI,CAC7BP,OAAO,CAACQ,IAAI,CAACC,SAAL,CAAeF,IAAf,CAAqB,IAArB,CAA2B,CAA3B,CAAD,CAAP,CACD,CAFD,EAGD,CALD,CAOAhB,SAAS,CAAC,UAAM,CACdK,OAAO,CAACc,WAAR,CAAoB,aAApB,CAAmC,iBAAMR,CAAAA,WAAW,CAAC,8BAAD,CAAjB,EAAnC,EACAN,OAAO,CAACc,WAAR,CAAoB,eAApB,CAAqC,iBAAMR,CAAAA,WAAW,CAAC,mBAAD,CAAjB,EAArC,EACAC,QAAQ,GACT,CAJQ,CAIN,EAJM,CAAT,CAMA,MACE,4BAAK,SAAS,CAAC,gBAAf,EACE,oBAAC,OAAD,EAAS,OAAO,CAAEP,OAAlB,EACE,4BAAM,SAAS,CAAC,UAAhB,uBADF,CAEE,2BAAK,SAAS,CAAC,YAAf,EACE,+BACE,gCAAOG,IAAP,CADF,CADF,CAFF,CAOE,2BAAK,SAAS,CAAC,WAAf,EACE,yBAAG,SAAS,CAAEE,QAAd,CAAwB,OAAO,CAAEE,QAAjC,YADF,CAPF,CADF,CADF,CAiBD,CArCD,CAuCA,cAAeR,CAAAA,eAAf,C","sourcesContent":["import React, { useState, useEffect } from 'react';\nimport { useFetcher, Fetcher } from 'use-fetcher-react';\nimport axios from 'axios';\n\nconst ListenerExample = () => {\n  const fetcher = useFetcher({\n    wrapperStyles: { display: 'none'}\n  });\n  const [json, setJson] = useState('No result yet');\n  const [btnClass, setBtnClass] = useState('button is-primary');\n\n  const loadData = () => {\n    let request = () => axios.get('https://api.chucknorris.io/jokes/random');\n    fetcher.fetch(request, data => {\n      setJson(JSON.stringify(data, null, 2));\n    });\n  };\n\n  useEffect(() => {\n    fetcher.addListener('fetch-start', () => setBtnClass('button is-primary is-loading'));\n    fetcher.addListener('all-fetch-end', () => setBtnClass('button is-primary'));\n    loadData();\n  }, []);\n\n  return (\n    <div className=\"test-container\">\n      <Fetcher fetcher={fetcher}>\n        <span className=\"my-title\">Random JSON loader</span>\n        <div className=\"my-content\">\n          <pre>\n            <code>{json}</code>\n          </pre>\n        </div>\n        <div className=\"my-footer\">\n          <a className={btnClass} onClick={loadData}>\n            Refresh\n          </a>\n        </div>\n      </Fetcher>\n    </div>\n  );\n};\n\nexport default ListenerExample;\n"]}]}