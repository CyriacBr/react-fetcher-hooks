{
  "name": "use-fetcher-react-docs",
  "homepage": "https://CyriacBr.github.io/react-use-fetcher",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "bulma": "^0.7.5",
    "prop-types": "^15.6.2",
    "raw-loader": "^3.0.0",
    "react": "../node_modules/react/",
    "react-dom": "../node_modules/react-dom/",
    "react-scripts-ts": "^2.16.0",
    "use-fetcher-react": ".."
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "docz:dev": "docz dev --typescript --port=4838",
    "docz:build": "docz build",
    "switch:local": "rm package.json && cp package-local.json package.json && yarn install && yarn start",
    "switch:prod": "rm package.json && cp package-prod.json package.json && yarn install && yarn docz:dev"
  },
  "devDependencies": {
    "docz": "latest",
    "docz-theme-default": "latest"
  },
  "resolutions": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
